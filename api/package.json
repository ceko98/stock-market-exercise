{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn ts-node-dev src/index.ts",
    "build": "rm -rf dist && tsc -p .",
    "db:up": "yarn build && knex --knexfile ./dist/knexfile.js migrate:latest",
    "db:down": "yarn build && knex --knexfile ./dist/knexfile.js migrate:rollback"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "express": "^4.18.2",
    "knex": "^2.3.0",
    "objection": "^3.0.1",
    "sqlite3": "^5.1.2",
    "typescript-rest": "^3.0.4"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
